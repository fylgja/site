{
  "name": "fylgja-site",
  "version": "1.0.1",
  "description": "The Fylgja site powered by eleventy",
  "private": true,
  "scripts": {
    "debug": "DEBUG=* npx eleventy",
    "clean": "rm -r _site/ || true",
    "test:css": "stylelint src/**/*.scss",
    "test": "npm run test:css",
    "start": "npm run dev",
    "dev:js": "npx rollup -c -w",
    "dev:css": "npx gulp dev",
    "dev": "npm run dev:css | npm run dev:js | eleventy --quiet --serve",
    "build:js": "NODE_ENV=production npx rollup -c",
    "build:css": "NODE_ENV=production npx gulp",
    "build": "ELEVENTY_ENV=prod eleventy && npm run build:css && npm run build:js",
    "prebuild": "npm test && npm run clean"
  },
  "keywords": [],
  "author": "fylgja",
  "license": "MIT",
  "stylelint": {
    "extends": "@fylgja/stylelint-config/scss",
    "rules": {
      "selector-max-id": 1
    }
  },
  "browserslist": [
    "last 1 version",
    "> 2%",
    "not dead",
    "last 3 iOS major versions",
    "not IE 11"
  ],
  "dependencies": {
    "@fylgja/aria-only": "^1.0.0",
    "@fylgja/aspect-ratio": "^1.0.0",
    "@fylgja/auto-grid": "^1.0.1",
    "@fylgja/avatar": "^1.0.0",
    "@fylgja/badge": "^1.0.0",
    "@fylgja/base": "^1.0.0",
    "@fylgja/breadcrumbs": "^1.0.0",
    "@fylgja/button": "^1.0.0",
    "@fylgja/card": "^1.0.0",
    "@fylgja/code": "^1.0.0",
    "@fylgja/container": "^1.0.0",
    "@fylgja/details": "^1.0.0",
    "@fylgja/dialog": "^1.0.0",
    "@fylgja/elevation": "^1.0.0",
    "@fylgja/form": "^1.0.0",
    "@fylgja/hashlink": "^1.0.0",
    "@fylgja/list": "^1.0.0",
    "@fylgja/menu": "^1.0.0",
    "@fylgja/print": "^1.0.0",
    "@fylgja/scroll-slider": "^1.0.0",
    "@fylgja/scrollbar": "^1.0.0",
    "@fylgja/section": "^1.0.0",
    "@fylgja/table": "^1.0.0",
    "@fylgja/utilpack": "^1.0.0",
    "clean-css": "^5.1.3",
    "dialog-polyfill": "^0.5.6",
    "docsearch.js": "^2.6.3",
    "prismjs": "^1.23.0",
    "roving-ux": "^1.0.1"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-img": "^0.9.0",
    "@11ty/eleventy-plugin-rss": "^1.1.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.1",
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@fylgja/fontface": "^2.1.1",
    "@fylgja/sass": "^1.0.1",
    "@fylgja/stylelint-config": "^3.4.0",
    "@quasibit/eleventy-plugin-sitemap": "^2.1.4",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "cssnano": "^5.0.4",
    "eleventy-plugin-nesting-toc": "^1.3.0",
    "eleventy-plugin-svg-contents": "^0.7.0",
    "eleventy-plugin-youtube-embed": "^1.6.3",
    "fibers": "^5.0.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-postcss": "^9.0.0",
    "gulp-sass": "^4.1.0",
    "html-minifier": "^4.0.0",
    "markdown-it-anchor": "^7.1.0",
    "markdown-it-external-anchor": "^1.0.0",
    "postcss": "^8.3.0",
    "postcss-preset-env": "^6.7.0",
    "rollup": "^2.50.2",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.34.0",
    "slugify": "^1.5.3",
    "stylelint": "^13.13.1",
    "terser": "^5.7.0"
  }
}
