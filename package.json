{
  "name": "fylgja-site",
  "version": "1.1.0",
  "description": "The Fylgja site powered by eleventy",
  "private": true,
  "scripts": {
    "debug": "DEBUG=* npx eleventy",
    "clean": "rm -r _site/ || true",
    "test:css": "stylelint src/**/*.scss",
    "test": "npm run test:css",
    "start": "npm run dev",
    "dev:js": "npx rollup -c -w",
    "dev:css": "npx gulp dev",
    "dev": "npm run dev:css | npm run dev:js | eleventy --quiet --serve",
    "build:js": "NODE_ENV=production npx rollup -c",
    "build:css": "NODE_ENV=production npx gulp",
    "build": "ELEVENTY_ENV=prod eleventy && npm run build:css && npm run build:js",
    "prebuild": "npm test && npm run clean"
  },
  "keywords": [],
  "author": "fylgja",
  "license": "MIT",
  "stylelint": {
    "extends": "@fylgja/stylelint-config/scss",
    "rules": {
      "selector-max-id": 1
    }
  },
  "browserslist": [
    "last 1 version",
    "> 2%",
    "not dead",
    "last 3 iOS major versions",
    "not IE 11"
  ],
  "dependencies": {
    "@docsearch/js": "^3.0.0-alpha.42",
    "clean-css": "^5.2.1",
    "dialog-polyfill": "^0.5.6",
    "docsearch.js": "^2.6.3",
    "fylgja": "^1.1.3",
    "prismjs": "^1.25.0",
    "roving-ux": "^1.0.2"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-img": "^1.0.0",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.3",
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@fylgja/fontface": "^2.1.1",
    "@fylgja/sass": "^2.0.0",
    "@fylgja/stylelint-config": "^4.0.1",
    "@quasibit/eleventy-plugin-sitemap": "^2.1.4",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "cssnano": "^5.0.15",
    "eleventy-plugin-nesting-toc": "^1.3.0",
    "eleventy-plugin-svg-contents": "^0.7.0",
    "eleventy-plugin-youtube-embed": "^1.6.4",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-postcss": "^9.0.1",
    "gulp-sass": "^5.0.0",
    "html-minifier": "^4.0.0",
    "markdown-it-anchor": "^8.4.1",
    "markdown-it-external-anchor": "^1.0.0",
    "postcss": "^8.3.9",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^7.0.1",
    "rollup": "^2.58.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.43.2",
    "slugify": "^1.6.1",
    "stylelint": "^14.1.0"
  }
}
